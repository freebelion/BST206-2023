<Window x:Class="Canvas1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Canvas1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Line
            X1="10" Y1="10"
            X2="50" Y2="50"
            Stroke="Black"
            StrokeThickness="4" />

        <Line
            X1="60" Y1="50"
            X2="60" Y2="10"
            Stroke="Black"
            StrokeThickness="4" />

        <Line
            X1="70" Y1="50"
            X2="110" Y2="10"
            Stroke="Black"
            StrokeThickness="4" />

        <Rectangle Width="150" Height="60" Fill="Red"
                   Canvas.Left="90" Canvas.Top="90"
                   Stroke="Black" StrokeThickness="2">
            <Rectangle.RenderTransform>
                <RotateTransform Angle="30"/>
            </Rectangle.RenderTransform>
        </Rectangle>

        <Ellipse Width="90" Height="120" Fill="Yellow"
                   Canvas.Left="250" Canvas.Top="10"/>
    </Canvas>
</Window>
